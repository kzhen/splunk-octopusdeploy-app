<dashboard script="autodiscover.js, last3monthsdeployments.js, activeProjectsByDeployments.js, tentacleHealth.js, activeUsers.js, avgDeploymentTime.js"
  stylesheet="octopusdeploy.css, nv.d3.min.css">
  <label>Overview</label>
  <row>
    <panel>
      <html>
        <h2>Active Environments (Last 3 Months)</h2>
        <div id="last3monthsdeployments">
            <svg id="last3monthsDeploymentsChart"></svg>
        </div>
      </html>
    </panel>
    <panel>
      <html>
        <h2>Active Projects (Last 3 Months)</h2>
        <div id="activeProjectsByDeployments">
          <svg id="activeProjectsByDeploymentsChart"></svg>
        </div>
      </html>
    </panel>
  </row>
  <row>
    <panel>
      <html>
        <h2>Tentacle Health</h2>
        <div id="tentacleHealth">
            <svg id="tentacleHealthChart"></svg>
        </div>
      </html>
    </panel>
    <panel>
      <html>
        <h2>Avgerage Deployment Time (seconds)</h2>
        <div id="avgDeploymentTime">
            <svg id="avgDeploymentTimeChart"></svg>
        </div>
      </html>
    </panel>
    <panel>
      <html>
        <h2>Active Users</h2>
        <div id="activeUsers">
          <svg id="activeUsersChart"></svg>
        </div>
      </html>
    </panel>
  </row>
  <row >
    <html>
      <div>
        Add some info/description
      </div>
    </html>
  </row>
  <row>
    <single id="avgDepSearchSingle">
      <search id="avgDepSearch" ref="OctopusDeploy - AvgDuration">
      </search>
    </single>
  </row>
</dashboard>
