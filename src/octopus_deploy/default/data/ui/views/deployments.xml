<dashboard script="autodiscover.js, deploymentDuration.js, deploymentOutcomeByProject.js, deploymentPunchCard.js"
  stylesheet="octopusdeploy.css, nv.d3.min.css, cal-heatmap.css">
  <label>Deployments</label>
  <search id="durationGroupedSearch" ref="OctopusDeploy - DurationGrouped"></search>
  <search id="deploymentByDay" ref="OctopusDeploy - DeploymentsByDay"></search>
  <row >
    <html>
      <div>
        Add some info/description
      </div>
    </html>
  </row>
  <row>
    <panel>
      <html>
        <h2>Punch Card</h2>
        <div id="punchCard">
          <div id="cal-heatmap"></div>
        </div>
      </html>
    </panel>
  </row>
  <row>
    <panel>
      <html>
        <h2>Deployment Outcome by Project</h2>
        <div id="deploymentOutcomeByProject">
          <svg id="deploymentOutcomeByProjectChart"></svg>
        </div>
      </html>
    </panel>
  </row>
  <row>

    <panel>
      <table>
        <title>Longest Releases through Environments</title>
        <search id="longestReleases" ref="OctopusDeploy - LongestReleases"></search>
        <option name="wrap">true</option>
        <option name="rowNumbers">false</option>
        <option name="dataOverlayMode">none</option>
        <option name="drilldown">cell</option>
        <option name="count">10</option>
      </table>
    </panel>
  </row>

  <row>
    <panel>
      <html>
        <h2>By Duration</h2>
        <div id="byDuration">
          <svg id="byDurationChart"></svg>
        </div>
      </html>
    </panel>
  </row>

</dashboard>
